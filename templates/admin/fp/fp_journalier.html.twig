
    <div class="row">
      <div class="col-sm-12">
        <div class="card-box table-responsive">
          <p class="text-muted font-13 m-b-30">
            Liste des feuille de présence
          </p>
          <table id="datatable-buttons" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th class="text-center">Id</th>
                    <th class="text-center">Numéro</th>
                    <th class="text-center">Service</th>
                    <th class="text-center">Ouverture/Clôture</th>
                    <th class="text-center">Informations</th>
                </tr>
            </thead>
            <tbody>
            {% for feuille_presence in feuille_presences %}
                <tr>
                    <td class="text-center">{{ feuille_presence.id }}</td>
                    <td class="text-center">{{ feuille_presence.numeroFeuille }}</td>
                    <td class="text-center">{{ feuille_presence.serviceDepartemental.nomSdc }}</td>
                    <td class="text-center">
                      {% if feuille_presence.active == true %}
                        <a type="button" class="link" data-toggle="modal" data-target=".bs-example-modal-lg" class="text-decoration-none float-left m-1" title="Supprimer"> <i class="fas fa-unlock text-success"></i> </a>
                        {% include "./admin/fp/disable.html.twig" %}
                      {% else %}
                          <i class="fas fa-lock text-danger"></i>
                      {% endif %}
                    </td>
                    <td class="text-center">
                      <div class="row">
                          <div class="col-md-8 mx-auto">
                              <a href="{{ path('app_admin_fp_show', {'id': feuille_presence.id}) }}"><i class="fas fa-eye text-info" title="Détails"></i></a>
                          </div>
                      </div>
                    </td>
                </tr>
            {% else %}
                <tr>
                  <td class="text-center" colspan="5">
                      <a href="{{ path('app_admin_fp_new') }}" title="Ajout feuille de présence" >Ajouter <i class="fas fa-fw fa-plus-circle"></i></a>
                  </td>
                </tr>
            {% endfor %}
            </tbody>
          </table>
        </div>
      </div>
    </div>